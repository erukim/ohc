{
  "name": "ohc",
  "version": "0.0.1",
  "types": "./typings/index.d.ts",
  "description": "OHC BOT Code",
  "main": "build/src/index.js",
  "scripts": {
    "start": "yarn run build && cd build && node apps/code/index.js",
    "start:pm2": "yarn run build && yarn run pm2:start",
    "dev": "ts-node-dev --transpile-only --respawn --trace-warnings --trace-uncaught --exit-child -- ./apps/code/index.ts",
    "generate": "prisma generate",
    "build": "echo 'OHC Bot의 빌드 작업을 시작합니다.' && tsc --project tsconfig.json && echo 'OHC Bot의 빌드 작업이 완료되었습니다. /build'",
    "test": "yarn run lint:fix && yarn run format:fix && yarn run build",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "update": "yarn upgrade-interactive",
    "pm2:start": "cd build && pm2 start ../pm2.json",
    "pm2:stop": "pm2 stop pm2.json",
    "pm2:delete": "pm2 delete pm2.json",
    "pm2:reload": "pm2 reload pm2.json",
    "pm2:restart": "pm2 restart pm2.json"
  },
  "author": "",
  "license": "GPL-3.0",
  "dependencies": {
    "@prisma/client": "^4.14.1",
    "@yimura/scraper": "^1.2.4",
    "aki-api": "^6.0.9",
    "axios": "^1.4.0",
    "canvas": "^2.11.2",
    "chalk": "^4.1.2",
    "cheerio": "^1.0.0-rc.12",
    "child_process": "^1.0.2",
    "comma-number": "^2.1.0",
    "discord-html-transcripts": "^3.1.4",
    "discord-together": "^1.3.31",
    "discord-xp": "^1.1.16",
    "discord.js": "^14.11.0",
    "dokdo": "^0.6.2",
    "dotenv": "^16.0.3",
    "erela.js": "https://github.com/Tomato6966/erela.js.git#commit=5139b6c2341fdd90652d852d9f6ef0d21f710571",
    "google-it": "^1.6.4",
    "hanspell": "^0.9.7",
    "korcen": "^0.2.4",
    "laftel.js": "^1.1.1",
    "lol-discord": "^0.2.2",
    "lyrics-finder": "^21.7.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "mongoose": "^6.6.5",
    "node-fetch": "^2.6.7",
    "qr-maker": "^2.0.0",
    "random-id": "^1.0.4",
    "request": "^2.88.2",
    "spotify-web-api-node": "^5.0.2",
    "strip-ansi": "^6.0.1",
    "unofficial-valorant-api": "^2.3.0-stable.0",
    "uuid": "^9.0.0",
    "weather-js": "^2.0.0",
    "winston": "^3.8.2"
  },
  "eslintIgnore": [
    "node_modules",
    "code/commands/",
    "example.*",
    "eslintrc.*",
    "build/"
  ],
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "packageManager": "yarn@3.5.0",
  "devDependencies": {
    "@types/cheerio": "^0.22.31",
    "@types/comma-number": "^2.1.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^18.7.13",
    "@types/node-fetch": "^2.6.4",
    "@types/node-schedule": "^2.1.0",
    "@types/nodemailer": "^6.4.5",
    "@types/randomstring": "^1.1.8",
    "@types/request-promise": "^4.1.48",
    "@types/urlencode": "^1.1.2",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.7.1",
    "prisma": "^4.14.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  }
}
